   <children>
      <ComboBox fx:id="comboBoxPerson" layoutX="40.0" layoutY="45.0" onAction="#onComboBoxPersonAction" prefWidth="150.0" promptText="Selecione a pessoas..." />
      <Button fx:id="btAll" layoutX="264.0" layoutY="45.0" mnemonicParsing="false" onAction="#onBtAllAction" text="All" />
   </children>
</AnchorPane>
